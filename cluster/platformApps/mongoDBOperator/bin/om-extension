function createFull {

	if [[ $# -eq 0 ]]; then
		exit 0
	fi
	
	create ${1} ${2} "crds.yaml" 
	echo "create crds"
	create ${1} ${2} "mongodb-enterprise.yaml"
	echo "create mongodb operator"
	create ${1} ${2}  "ops-manager-secret.yaml"
	echo "created secret"
	create ${1} ${2}  "ops-manager.yaml"
	echo "created ops manager"
}
